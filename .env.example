# LinkedIn OAuth Configuration
# Get these from your LinkedIn Developer Application: https://developer.linkedin.com/

# LinkedIn App Credentials
LINKEDIN_CLIENT_ID=your_client_id_here
LINKEDIN_CLIENT_SECRET=your_client_secret_here

# OAuth Tokens
LINKEDIN_ACCESS_TOKEN=your_access_token_here
LINKEDIN_REFRESH_TOKEN=your_refresh_token_here

# Optional: ID Token (for OpenID Connect)
LINKEDIN_ID_TOKEN=your_id_token_here

# Redirect URI (must match your app configuration)
LINKEDIN_REDIRECT_URI=http://localhost:8080/callback 